<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:toolbar="http://schemas.android.com/apk/res-auto"
    android:background="@color/grey_c"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.sky31.gonggong.module.swzl.search.SwzlSearchActivity">


    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_0176da"
        toolbar:navigationIcon="@drawable/ic_arrow_back"
        android:id="@+id/swzl_search_toolbar"
        toolbar:title=" "
        >

        <EditText
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:inputType="text"
            android:id="@+id/swzl_search_edit_text"
            />


    </android.support.v7.widget.Toolbar>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:dividerHeight="@dimen/interval_10"
        android:id="@+id/swzl_search_result">

    </ListView>

</LinearLayout>
